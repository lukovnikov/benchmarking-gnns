{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MASNN",
    "dataset": "ZINC",
    
    "out_dir": "out/molecules_graph_regression/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 128,
        "init_lr": 0.0005,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 5,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 2,
        "numsublayers": 4,
        "numlayers": 2,
        "numechos": 2,
        "hidden_dim": 80,
        "out_dim": 80,
        "edge_feat": true,
        "readout": "max",
        "dropout": 0.0,
        "batch_norm": true,
        "pos_enc": false,
        "n_heads": 2,
        "use_sgru": false
    }
}